TARGETTYPE dll
TARGET ${{PREFIX}}_logs.pyd

CAPABILITY ${{DLL_CAPABILITIES}}

${{COMPILER_FLAGS}}

SYSTEMINCLUDE    \epoc32\include\python25 
SYSTEMINCLUDE    \epoc32\include\stdapis 
SYSTEMINCLUDE    \epoc32\include 

USERINCLUDE   ..\inc 


LIBRARY python25.lib
LIBRARY euser.lib 
LIBRARY logcli.lib 
LIBRARY logwrap.lib
LIBRARY flogger.lib
LIBRARY efsrv.lib 


SOURCEPATH ..\src

SOURCE      logging.cpp
SOURCE      eventarray.cpp
SOURCE      logengine.cpp
SOURCE      container.cpp
SOURCE      eventdata.cpp

#if defined(ARMCC)
deffile eabi\_logs 
#elif defined(WINSCW)
deffile bwins\_logs 
#endif
